.container {
	position: relative;
	height: 479px;
	font-size: 12px;
	width: 280px;
	border: 1px solid #dedede;
	background-color: #f6f6f6;
	font-family: Arial;
}

.header {
	line-height: 36px;
	width: 100%;
	border-bottom: 1px dashed #dadada;
	text-align: center;
	position: relative;
	color: #5f5f5f;
	font-weight: bold;
	text-transform: uppercase;
}

.body {
	padding-left: 5px;
	padding-right: 5px;
}